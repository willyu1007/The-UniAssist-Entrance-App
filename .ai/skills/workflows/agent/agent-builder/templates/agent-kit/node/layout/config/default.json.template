{
  "agent_id": "{{agent_id}}",
  "base_path": "{{api_base_path}}",
  "ws_path": "{{api_base_path}}/ws",
  "sse_path": "{{api_base_path}}/run/stream",
  "http": {
    "port_env": "PORT",
    "auth_header": "authorization",
    "max_request_size_bytes": 1048576
  },
  "logging": {
    "level": "info"
  },
  "tools": {
    "enabled": false,
    "max_steps": 5
  },
  "llm": {
    "timeout_ms": 60000
  },
  "worker": {
    "max_concurrency": 4,
    "poll_ms": 1000
  },
  "conversation": {
    "max_items": 1000,
    "ttl_seconds": 86400
  }
}
